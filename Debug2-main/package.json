{
  "name": "debug",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:challenge": "jest --testPathPattern=challenge",
    "test:api": "jest --testPathPattern=api"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.25.5",
    "@google/generative-ai": "^0.24.1",
    "d3": "^7.9.0",
    "mongoose": "^8.16.4",
    "next": "15.4.2",
    "node-fetch": "^3.3.2",
    "pdfkit": "^0.17.1",
    "postcss": "^8.5.6",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-syntax-highlighter": "^15.6.1",
    "reactflow": "^11.11.4",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "mongodb-memory-server": "^9.5.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.js"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/**/*.test.{js,jsx}",
      "!src/**/index.{js,jsx}"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@testing-library|jest)/)"
    ]
  }
}
